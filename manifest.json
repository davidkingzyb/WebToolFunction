{

    "manifest_version": 2,
    "name": "WebToolFunction",
    "version": "1.1",
    "description": "WTF is it?",
    "options_page":"index.html",
    "background":{"page":"background.html"},
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon19.png",
            "38": "images/icon38.png"
        },
        "default_title": "WebToolFunction"
        // "default_popup": "chrome.html"
    },
    "permissions": [
        "tts", "tabs", "*://*/*", "cookies" ,"storage","webRequest"
    ],
    "content_scripts": [{
        "matches": ["*://*/*"], 
        "js": ["wtf.js","terminal.js","chrome.js"]
    }]
}